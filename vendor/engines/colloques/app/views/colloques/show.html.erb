<% content_for :body_content_left do %>
  <h2><%= @colloque.title %></h2>
  <%=raw @colloque.contenu %>
<% end %>

<% content_for :body_content_right do %>
  <aside class="liste colloques">
    <h2><%= link_to "Autres colloques", colloques_path %></h2>
    <%= render 'other', :colloques => @other_colloques %>
  </aside>
<% end %>

<%= render :partial => "/shared/content_page" %>
